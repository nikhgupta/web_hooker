<div class="page-header">
  <%= link_to destinations_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_destination_path(@destination), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show destination</h1>
</div>

<dl class="dl-horizontal">
  <dt>Portal:</dt>
  <dd><%= @destination.portal %></dd>

  <dt>Url:</dt>
  <dd><%= @destination.url %></dd>

</dl>
